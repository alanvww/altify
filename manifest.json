{
	"manifest_version": 3,
	"name": "Draggablify",
	"version": "1.0",
	"description": "Changes the background color of the webpage",
	"permissions": ["activeTab", "scripting"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": {}
	}
}
